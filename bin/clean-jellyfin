#!/usr/bin/env bash
set -Eeuo pipefail

if ((EUID != 0)); then
    echo 'This script is dangerous and requires super-user privileges!' >&2
    exit 1
fi

cd "$(dirname "$(dirname "${BASH_SOURCE[0]}")")"
shopt -s extglob
rm -fr \
    data/jellyfin/*/ \
    services/jellyfin/!(config|transcodes)/ \
    services/jellyfin/config/!(dlna)/ \
    services/jellyfin/config/dlna/!(user)/
